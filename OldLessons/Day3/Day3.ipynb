       "\n",
       "\n",
       "<h3>Examples</h3>\n",
       "\n",
       "<pre>\n",
       "require(graphics)\n",
       "dotchart(precip[order(precip)], main = \"precip data\")\n",
       "title(sub = \"Average annual precipitation (in.)\")\n",
